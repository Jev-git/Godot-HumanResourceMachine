; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Box",
"language": "GDScript",
"path": "res://Scripts/Box.gd"
}, {
"base": "Area2D",
"class": "Instruction",
"language": "GDScript",
"path": "res://Scripts/Instruction.gd"
}, {
"base": "Node2D",
"class": "RandomSolver",
"language": "GDScript",
"path": "res://Scripts/RandomSolver.gd"
}, {
"base": "Area2D",
"class": "SourceInstruction",
"language": "GDScript",
"path": "res://Scripts/SourceInstruction.gd"
}, {
"base": "Node2D",
"class": "Verifier",
"language": "GDScript",
"path": "res://Scripts/Verifier.gd"
} ]
_global_script_class_icons={
"Box": "",
"Instruction": "",
"RandomSolver": "",
"SourceInstruction": "",
"Verifier": ""
}

[application]

config/name="Human Resource Machine"
run/main_scene="res://Scenes/Room.tscn"

[autoload]

InstructionType="*res://Scripts/InstructionType.gd"
NodeUtil="*res://Scripts/NodeUtil.gd"

[display]

window/size/width=576
window/size/height=1024
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

common/enable_pause_aware_picking=true
